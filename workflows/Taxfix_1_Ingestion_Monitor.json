{
    "name": "Taxfix_1_Ingestion_Monitor",
    "nodes": [
        {
            "parameters": {
                "rule": {
                    "interval": [
                        {
                            "field": "hours"
                        }
                    ]
                }
            },
            "name": "Schedule Trigger",
            "type": "n8n-nodes-base.scheduleTrigger",
            "typeVersion": 1,
            "position": [
                250,
                300
            ],
            "id": "schedule-trigger"
        },
        {
            "parameters": {
                "url": "https://www.bundesfinanzministerium.de/Content/DE/RSS/Steuern/steuern.xml",
                "options": {
                    "response": {
                        "response": {
                            "responseFormat": "text"
                        }
                    }
                },
                "headerParameters": {
                    "parameters": [
                        {
                            "name": "User-Agent",
                            "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
                        }
                    ]
                }
            },
            "name": "Fetch BMF (HTTP)",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 3,
            "position": [
                450,
                200
            ],
            "id": "fetch-bmf-http"
        },
        {
            "parameters": {
                "url": "https://www.finanztip.de/feed/",
                "options": {
                    "response": {
                        "response": {
                            "responseFormat": "text"
                        }
                    }
                },
                "headerParameters": {
                    "parameters": [
                        {
                            "name": "User-Agent",
                            "value": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
                        }
                    ]
                }
            },
            "name": "Fetch Finanztip (HTTP)",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 3,
            "position": [
                450,
                400
            ],
            "id": "fetch-finanztip-http"
        },
        {
            "parameters": {
                "jsCode": "// Simple XML Regex Parser for BMF\n// Because built-in nodes struggle with strict XML\nconst xml = $json.data || '';\nconst items = [];\n// Extract items\nconst itemRegex = /<item>([\\s\\S]*?)<\\/item>/g;\nlet match;\nwhile ((match = itemRegex.exec(xml)) !== null) {\n  const content = match[1];\n  const title = (content.match(/<title>([\\s\\S]*?)<\\/title>/) || [])[1] || '';\n  const link = (content.match(/<link>([\\s\\S]*?)<\\/link>/) || [])[1] || '';\n  const desc = (content.match(/<description>([\\s\\S]*?)<\\/description>/) || [])[1] || '';\n  // Unescape CDATA if present\n  const cleanTitle = title.replace('<![CDATA[', '').replace(']]>', '').trim();\n  const cleanLink = link.trim();\n  const cleanDesc = desc.replace('<![CDATA[', '').replace(']]>', '').trim();\n\n  if (cleanTitle) {\n    items.push({\n      json: {\n        title: cleanTitle,\n        link: cleanLink,\n        contentSnippet: cleanDesc,\n        source: 'BMF'\n      }\n    });\n  }\n}\nreturn items;"
            },
            "name": "Parse BMF XML",
            "type": "n8n-nodes-base.code",
            "typeVersion": 1,
            "position": [
                600,
                200
            ],
            "id": "parse-bmf"
        },
        {
            "parameters": {
                "jsCode": "// Simple XML Regex Parser for Finanztip\nconst xml = $json.data || '';\nconst items = [];\nconst itemRegex = /<item>([\\s\\S]*?)<\\/item>/g;\nlet match;\nwhile ((match = itemRegex.exec(xml)) !== null) {\n  const content = match[1];\n  const title = (content.match(/<title>([\\s\\S]*?)<\\/title>/) || [])[1] || '';\n  const link = (content.match(/<link>([\\s\\S]*?)<\\/link>/) || [])[1] || '';\n  const desc = (content.match(/<description>([\\s\\S]*?)<\\/description>/) || [])[1] || '';\n  \n  const cleanTitle = title.replace('<![CDATA[', '').replace(']]>', '').trim();\n  const cleanLink = link.trim();\n  const cleanDesc = desc.replace('<![CDATA[', '').replace(']]>', '').trim();\n\n  if (cleanTitle) {\n    items.push({\n      json: {\n        title: cleanTitle,\n        link: cleanLink,\n        contentSnippet: cleanDesc,\n        source: 'Finanztip'\n      }\n    });\n  }\n}\nreturn items;"
            },
            "name": "Parse Finanztip XML",
            "type": "n8n-nodes-base.code",
            "typeVersion": 1,
            "position": [
                600,
                400
            ],
            "id": "parse-finanztip"
        },
        {
            "parameters": {
                "mode": "append"
            },
            "name": "Merge Feeds",
            "type": "n8n-nodes-base.merge",
            "typeVersion": 1,
            "position": [
                750,
                300
            ],
            "id": "merge-feeds"
        },
        {
            "parameters": {
                "resource": "chat",
                "model": "gpt-4o",
                "jsonOutput": true,
                "messages": {
                    "values": [
                        {
                            "role": "system",
                            "content": "You are a tax content analyst for Taxfix (Target: Young employees/students 25-35).\nDecide if this news item is relevant for TikTok/Instagram content.\n\nRELEVANT: Deductions, Deadlines, Benefits, Home Office, Crypto, ETF, Student Tips.\nIRRELEVANT: Corporate tax, Bureaucracy, Stats, International Treaties.\n\nReturn JSON:\n{\n  \"relevant\": true/false,\n  \"topic\": \"Engaging German title\",\n  \"confidence\": 0.0-1.0,\n  \"platform\": \"TikTok\",\n  \"reasoning\": \"Why relevant?\"\n}"
                        },
                        {
                            "role": "user",
                            "content": "News Item:\nTitle: {{ $json.title }}\nSnippet: {{ $json.contentSnippet }}\nLink: {{ $json.link }}\nAnalyze:"
                        }
                    ]
                }
            },
            "name": "Analyze Relevance",
            "type": "n8n-nodes-base.openAi",
            "typeVersion": 1,
            "position": [
                950,
                300
            ],
            "id": "analyze-relevance",
            "credentials": {
                "openAiApi": {
                    "id": "openai_account",
                    "name": "OpenAI account"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $json.relevant }}",
                            "value2": true
                        }
                    ],
                    "number": [
                        {
                            "value1": "={{ $json.confidence }}",
                            "operation": "larger",
                            "value2": 0.7
                        }
                    ]
                }
            },
            "name": "Filter High Quality",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                1150,
                300
            ],
            "id": "filter-quality"
        },
        {
            "parameters": {
                "operation": "getAll",
                "tableId": "content_queue",
                "limit": 1,
                "filters": {
                    "conditions": [
                        {
                            "key": "topic",
                            "value": "={{ $json.topic }}",
                            "operator": "eq"
                        }
                    ]
                }
            },
            "name": "Check Duplicates",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1350,
                300
            ],
            "id": "check-dupes",
            "alwaysOutputData": true,
            "credentials": {
                "supabaseApi": {
                    "id": "supabase_api_creds",
                    "name": "Supabase API"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $items('Check Duplicates').length === 0 }}",
                            "value2": true
                        }
                    ]
                }
            },
            "name": "Is New?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                1550,
                300
            ],
            "id": "is-new"
        },
        {
            "parameters": {
                "operation": "create",
                "tableId": "content_queue",
                "columnsUi": {
                    "columnValues": [
                        {
                            "fieldId": "topic",
                            "fieldValue": "={{ $json.topic }}"
                        },
                        {
                            "fieldId": "source_url",
                            "fieldValue": "={{ $('Analyze Relevance').item.json.link }}"
                        },
                        {
                            "fieldId": "status",
                            "fieldValue": "PENDING_GENERATION"
                        },
                        {
                            "fieldId": "platform",
                            "fieldValue": "={{ $json.platform }}"
                        },
                        {
                            "fieldId": "compliance_score",
                            "fieldValue": "={{ $json.confidence }}"
                        }
                    ]
                }
            },
            "name": "Add to Queue",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1750,
                300
            ],
            "id": "add-to-queue",
            "credentials": {
                "supabaseApi": {
                    "id": "supabase_api_creds",
                    "name": "Supabase API"
                }
            }
        }
    ],
    "connections": {
        "Schedule Trigger": {
            "main": [
