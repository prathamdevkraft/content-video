{
    "name": "Taxfix_1_Ingestion_Monitor",
    "nodes": [
        {
            "parameters": {
                "rule": {
                    "interval": [
                        {
                            "field": "hours"
                        }
                    ]
                }
            },
            "name": "Schedule Trigger",
            "type": "n8n-nodes-base.scheduleTrigger",
            "typeVersion": 1,
            "position": [
                250,
                300
            ],
            "id": "schedule-trigger"
        },
        {
            "parameters": {
                "url": "https://www.bundesfinanzministerium.de/Content/DE/RSS/Steuern/steuern.xml"
            },
            "name": "Fetch BMF News",
            "type": "n8n-nodes-base.rssFeedRead",
            "typeVersion": 1,
            "position": [
                450,
                200
            ],
            "id": "fetch-rss-bmf"
        },
        {
            "parameters": {
                "url": "https://www.finanztip.de/feed/"
            },
            "name": "Fetch Finanztip",
            "type": "n8n-nodes-base.rssFeedRead",
            "typeVersion": 1,
            "position": [
                450,
                400
            ],
            "id": "fetch-rss-finanztip"
        },
        {
            "parameters": {
                "mode": "append"
            },
            "name": "Merge Feeds",
            "type": "n8n-nodes-base.merge",
            "typeVersion": 1,
            "position": [
                650,
                300
            ],
            "id": "merge-feeds"
        },
        {
            "parameters": {
                "resource": "chat",
                "model": "gpt-4o",
                "jsonOutput": true,
                "messages": {
                    "values": [
                        {
                            "role": "system",
                            "content": "You are a tax content analyst for Taxfix (Target: Young employees/students 25-35).\nDecide if this news item is relevant for TikTok/Instagram content.\n\nRELEVANT: Deductions, Deadlines, Benefits, Home Office, Crypto, ETF, Student Tips.\nIRRELEVANT: Corporate tax, Bureaucracy, Stats, International Treaties.\n\nReturn JSON:\n{\n  \"relevant\": true/false,\n  \"topic\": \"Engaging German title\",\n  \"confidence\": 0.0-1.0,\n  \"platform\": \"TikTok\",\n  \"reasoning\": \"Why relevant?\"\n}"
                        },
                        {
                            "role": "user",
                            "content": "News Item:\nTitle: {{ $json.title }}\nSnippet: {{ $json.contentSnippet }}\nLink: {{ $json.link }}\nAnalyze:"
                        }
                    ]
                }
            },
            "name": "Analyze Relevance",
            "type": "n8n-nodes-base.openAi",
            "typeVersion": 1,
            "position": [
                850,
                300
            ],
            "id": "analyze-relevance",
            "credentials": {
                "openAiApi": {
                    "id": "openai_account",
                    "name": "OpenAI account"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $json.relevant }}",
                            "value2": true
                        }
                    ],
                    "number": [
                        {
                            "value1": "={{ $json.confidence }}",
                            "operation": "larger",
                            "value2": 0.7
                        }
                    ]
                }
            },
            "name": "Filter High Quality",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                1050,
                300
            ],
            "id": "filter-quality"
        },
        {
            "parameters": {
                "operation": "getAll",
                "tableId": "content_queue",
                "limit": 1,
                "filters": {
                    "conditions": [
                        {
                            "key": "topic",
                            "value": "={{ $json.topic }}",
                            "operator": "eq"
                        }
                    ]
                }
            },
            "name": "Check Duplicates",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1250,
                300
            ],
            "id": "check-dupes",
            "alwaysOutputData": true,
            "credentials": {
                "supabaseApi": {
                    "id": "supabase_api_creds",
                    "name": "Supabase API"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $items('Check Duplicates').length === 0 }}",
                            "value2": true
                        }
                    ]
                }
            },
            "name": "Is New?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                1450,
                300
            ],
            "id": "is-new"
        },
        {
            "parameters": {
                "operation": "create",
                "tableId": "content_queue",
                "columnsUi": {
                    "columnValues": [
                        {
                            "fieldId": "topic",
                            "fieldValue": "={{ $json.topic }}"
                        },
                        {
                            "fieldId": "source_url",
                            "fieldValue": "={{ $('Analyze Relevance').item.json.link }}"
                        },
                        {
                            "fieldId": "status",
                            "fieldValue": "PENDING_GENERATION"
                        },
                        {
                            "fieldId": "platform",
                            "fieldValue": "={{ $json.platform }}"
                        },
                        {
                            "fieldId": "compliance_score",
                            "fieldValue": "={{ $json.confidence }}"
                        }
                    ]
                }
            },
            "name": "Add to Queue",
            "type": "n8n-nodes-base.supabase",
            "typeVersion": 1,
            "position": [
                1650,
                300
            ],
            "id": "add-to-queue",
            "credentials": {
                "supabaseApi": {
                    "id": "supabase_api_creds",
                    "name": "Supabase API"
                }
            }
        }
    ],
    "connections": {
        "Schedule Trigger": {
            "main": [
                [
                    {
                        "node": "Fetch BMF News",
                        "type": "main",
                        "index": 0
                    },
                    {
                        "node": "Fetch Finanztip",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Fetch BMF News": {
            "main": [
                [
                    {
                        "node": "Merge Feeds",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Fetch Finanztip": {
            "main": [
                [
                    {
                        "node": "Merge Feeds",
                        "type": "main",
                        "index": 1
                    }
                ]
            ]
        },
        "Merge Feeds": {
            "main": [
                [
                    {
                        "node": "Analyze Relevance",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Analyze Relevance": {
            "main": [
                [
                    {
                        "node": "Filter High Quality",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Filter High Quality": {
            "main": [
                [
                    {
                        "node": "Check Duplicates",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Check Duplicates": {
            "main": [
                [
                    {
                        "node": "Is New?",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Is New?": {
            "main": [
                [
                    {
                        "node": "Add to Queue",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    }
}