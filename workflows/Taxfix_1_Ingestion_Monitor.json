                    }
                ]
            ]
        },
        "Filter High Quality": {
            "main": [
                [
                    {
                        "node": "Check Duplicates",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Check Duplicates": {
            "main": [
                [
                    {
                        "node": "Is New?",
                        "type": "main",
